{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 10"
              ]
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "lodash"
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 10"
              ]
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "lodash"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 10"
              ]
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "lodash"
      ]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions",
                "ie >= 10"
              ]
            },
            "modules": false
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "lodash"
      ]
    }
  }
}
